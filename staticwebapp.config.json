{
    "routes": [
      {
        "route": "/api/*",
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/*",
        "allowedRoles": ["anonymous"]
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/api/*"]
    },
    "platform": {
      "apiRuntime": "python:3.11"
    },
    "globalHeaders": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
      "Access-Control-Allow-Headers": "Content-Type"
    }
  }